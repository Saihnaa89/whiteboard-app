{
  "name": "whiteboard-app",
  "version": "1.0.0",
  "private": true,
  "description": "A collaborative whiteboard application",
  "main": "server/src/index.js",
  "scripts": {
    "start": "node server/dist/index.js",
    "dev": "node server/dist/index.js",
    "build": "tsc -p server/tsconfig.json",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@types/winston": "^2.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "winston": "^3.17.0"
  }
}
